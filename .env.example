# Minikube Development Secrets
# Copy this file to .env and fill in with production values from DO cluster

# GitHub OAuth
GITHUB_CLIENT_SECRET=your-github-oauth-client-secret

# Email (Postmark)
ROCKETSHIP_EMAIL_FROM=your-email-from-address
ROCKETSHIP_POSTMARK_SERVER_TOKEN=your-postmark-server-token

# Database (optional, auto-generated if not set)
POSTGRES_PASSWORD=your-postgres-password

# GitHub App (for repo access)
ROCKETSHIP_GITHUB_APP_ID=your-github-app-id
ROCKETSHIP_GITHUB_APP_SLUG=your-github-app-slug
ROCKETSHIP_GITHUB_APP_PRIVATE_KEY_FILE=/path/to/private-key.pem

# GitHub Webhook (for webhook ingestion via smee.io relay)
ROCKETSHIP_GITHUB_WEBHOOK_SECRET=your-webhook-secret
ROCKETSHIP_GITHUB_WEBHOOK_SMEE_URL=https://smee.io/your-channel
