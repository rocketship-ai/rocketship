# Minikube Development Secrets
# Copy this file to .env and fill in with production values from DO cluster

# GitHub OAuth
GITHUB_CLIENT_SECRET=your-github-oauth-client-secret

# Email (Postmark)
ROCKETSHIP_EMAIL_FROM=your-email-from-address
ROCKETSHIP_POSTMARK_SERVER_TOKEN=your-postmark-server-token

# Database (optional, auto-generated if not set)
POSTGRES_PASSWORD=your-postgres-password
